(function(){
    angular
        .module('<%= application.name %>', ['ngRoute'])
        .config(configuration);

    function configuration($routeProvider) {
        $routeProvider
<%      for(var entityName in application.entities) {
%>
            .when('/<%= entityName %>', {
                templateUrl: '/wax/<%= application.name %>/<%= entityName %>/templates/list/<%= entityName %>-list.view.client.html',
                controller:  '<%= entityName %>ListController',
                controllerAs: 'model'
            })
            .when('/<%= entityName %>/:<%= entityName %>Id', {
                templateUrl: '/wax/<%= application.name %>/<%= entityName %>/templates/edit/<%= entityName %>-edit.view.client.html',
                controller:  '<%= entityName %>EditController',
                controllerAs: 'model'
            })
<%      }
%>
    }
})();